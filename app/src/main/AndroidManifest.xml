<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xample.masyadi.coffelate2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityFavorit" />
        <activity android:name=".ActivityPesanan" />
        <activity android:name=".ActivityAkun" />
        <activity
            android:name=".MenuDetailActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ActivityTentangApp" />
        <activity android:name=".ActivityCaraPenggunaan"></activity>
    </application>

</manifest>